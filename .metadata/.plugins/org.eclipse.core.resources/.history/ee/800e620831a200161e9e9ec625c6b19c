import java.util.Arrays;

public class LotsofCopies
{
	public static void main (String[] args)
	{	
			int num = 7;
			String strMain = "APCS";
			int [] arrMain = {1, 2, 3, 4, 5};
			System.out.print(Arrays.toString(arrMain));
			
		}
	public static void changeMe (int x, String str, int [] arr)
	{
		x = 7;
		str = "blah";
		for (int i = 0; i <= 4; i++){
		arr[i] = i;
		}
	}
}